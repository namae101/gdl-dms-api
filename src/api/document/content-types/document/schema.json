    "title": {
      "type": "string",
      "required": true
    },
    "number": {
      "type": "string",
      "required": true
    },
    "documentOriginalDate": {
      "type": "date"
    },
    "documentArrivedOrSentDate": {
      "type": "date"
    },
    "needUpdate": {
      "type": "boolean",
      "default": false,
      "required": false
