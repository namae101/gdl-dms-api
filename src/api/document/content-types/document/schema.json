      "target": "api::entity.entity",
      "inversedBy": "sentDocuments"
    },
    "recievingEntity": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::entity.entity",
      "inversedBy": "recievingDocuments"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "documents"
    },
    "odocuments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::document.document",
      "inversedBy": "rdocuments"
